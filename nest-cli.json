    "slack": {
      "type": "library",
      "root": "libs/slack",
      "entryFile": "index",
      "sourceRoot": "libs/slack/src",
      "compilerOptions": {
        "tsConfigPath": "libs/slack/tsconfig.lib.json"
      }
    }
